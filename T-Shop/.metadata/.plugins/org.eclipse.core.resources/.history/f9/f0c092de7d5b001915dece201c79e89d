package comp.utilizatori;

import java.io.Serializable;

public class Utilizator implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private int idUtilizator;
	private String username;
	private String password;
	private String status;

	public String getStatus() {
		return status;
	}

	public void setStatus(String rank) {
		this.status = rank;
	}

	public int getIdUtilizator() {
		return idUtilizator;
	}

	public void setIdUtilizator(int userId) {
		this.idUtilizator = userId;
	}

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public Utilizator(int idUser, String username, String password) {
		super();
		this.username = username;
		this.password = password;
	}

	@Override
	public String toString() {
		return "Utilizator [username=" + username + ", password=" + password + ", status=" + status + "]";
	}

	public Utilizator() {
		super();
	}

	public Utilizator(String username) {
		this.username = username;
	}

	public Utilizator(String username, String password, String rank) {
		super();
		this.username = username;
		this.password = password;
		this.setStatus(rank);
	}

}


